#!/bin/bash
if [ "$1" == "" ];
then
  echo '1st parameter needs to name of command'
  exit 1
fi

command_name=$1
name_of_app_in_applications_folder=$2
echo 'open -na '$name_of_app_in_applications_folder'.app "$@"' > $MODULAR_BASHING_BIN_DIR/$command_name
chmod +x $MODULAR_BASHING_BIN_DIR/$command_name
