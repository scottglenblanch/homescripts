#!/bin/bash
if [ "$1" == "" ] || [ "$2" == "" ]
then
  echo '1st parameter is name of command'
  echo '2nd parameter is name of application you want to kill'
  exit 1
fi
command_name=$1
app_name=$2

killCommandAsString="osascript -e 'quit app \""$app_name".app\"'"
echo $killCommandAsString > $MODULAR_BASHING_BIN_DIR/$command_name
chmod +x $MODULAR_BASHING_BIN_DIR/$command_name
