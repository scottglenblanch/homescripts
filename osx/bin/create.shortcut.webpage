#!/bin/bash
if [ "$1" == "" ] || [ "$2" == "" ];
then
  echo '1st parameter is command, 2nd parameter is website url to open'
  exit 1
fi

nameOfFile=$1
urlToGoTo=$2
echo '#!/bin/bash' > $nameOfFile
echo 'chrome '$2 >> $nameOfFile
echo '' >> $nameOfFile
chmod +x $nameOfFile
link $nameOfFile $HOME_BIN/$nameOfFile
setup.shell
