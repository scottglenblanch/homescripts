#!/bin/bash
if [ "$1" == "" ] || [ "$2" == "" ];
then
  echo '1st parameter is command, 2nd parameter is website url to open'
  exit 1
fi

command_name=$1
url=$2
echo '#!/bin/bash' > $command_name
echo 'chrome '$url >> $command_name
echo '' >> $command_name
chmod +x $command_name
link $command_name $MODULAR_BASHING_BIN_DIR/$command_name
